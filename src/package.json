    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.0",
    "@types/node": "^25.3.0",
    "eslint": "^9.22.0",
    "jsdom": "^28.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "posthog-js": "^1.352.0"
  }
}
